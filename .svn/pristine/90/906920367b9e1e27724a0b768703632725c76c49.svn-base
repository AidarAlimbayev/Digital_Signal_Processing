function [MODm,m] = fft_e2(MODX,e2)
% ќпределение значений модул€ ƒѕ‘ и частот полезного сигнала
%
% MODX Ч вектор значений модул€ ƒѕ‘ смеси сигнала с шумом
% e2 Ч заданный порог
%
% MODm Ч вектор значений модул€ ƒѕ‘ полезного сигнала
% m Ч вектор значений частот полезного сигнала
%
i = 1;
P = sum(MODX.^2)/length(MODX); % P Ч средн€€ мощность смеси сигнала с шумом
for k = 1:length(MODX)
   if ((MODX(k).^2)/P)>e2
      MODm(i) = MODX(k);
      m(i) = k-1;
      i = i+1;
    end
end



 

 